{
  "name": "vector-analytics",
  "version": "0.0.5",
  "description": "Minimal alternative to Google Analytics.",
  "author": "DSRKafuU <dsrkafuu@outlook.com> (https://dsrkafuu.su)",
  "license": "Apache-2.0",
  "scripts": {
    "postinstall": "husky install",
    "lint:web": "vue-cli-service lint",
    "lint:api": "eslint ./api",
    "lint": "npm-run-all -p lint:web lint:api",
    "dev:web": "cross-env NODE_ENV=development vue-cli-service serve",
    "dev:api": "cross-env NODE_ENV=development nodemon --watch src --watch api --watch utils ./api/index.js",
    "dev": "cross-env NODE_ENV=development npm-run-all -p dev:api dev:web",
    "build:web": "cross-env NODE_ENV=production vue-cli-service build",
    "build:api": "cross-env NODE_ENV=production node ./utils/build.js",
    "build": "cross-env NODE_ENV=production npm-run-all build:web build:api",
    "test:web": "cross-env NODE_ENV=production vue-cli-service build --report",
    "start": "cross-env NODE_ENV=production node ./api/index.js",
    "vercel-build": "cross-env NODE_ENV=production npm run build:api"
  },
  "dependencies": {
    "axios": "~0.21.1",
    "bcrypt": "~5.0.1",
    "bowser": "~2.11.0",
    "chart.js": "3.0.0-beta.11",
    "chartjs-chart-geo": "3.0.0-beta.9",
    "compression": "~1.7.4",
    "connect-history-api-fallback": "~1.6.0",
    "cookie-parser": "~1.4.5",
    "core-js": "~3.9.0",
    "cors": "~2.8.5",
    "express": "5.0.0-alpha.8",
    "is-localhost-ip": "~1.4.0",
    "isbot": "~3.0.24",
    "js-cookie": "~2.2.1",
    "jsonwebtoken": "~8.5.1",
    "lodash": "~4.17.21",
    "maxmind": "~4.3.1",
    "mongoose": "~5.11.18",
    "normalize.css": "~8.0.1",
    "on-headers": "~1.0.2",
    "serverless-http": "~2.7.0",
    "vue": "~2.6.12",
    "vue-router": "~3.5.1",
    "vuex": "~3.6.2"
  },
  "lint-staged": {
    "*.{js,json,css,scss,md}": "prettier --write"
  },
  "devDependencies": {
    "@babel/core": "^7.13.8",
    "@babel/preset-env": "^7.13.8",
    "@commitlint/cli": "^12.0.1",
    "@commitlint/config-conventional": "^12.0.1",
    "@vue/cli-plugin-babel": "^4.5.11",
    "@vue/cli-plugin-eslint": "^4.5.11",
    "@vue/cli-plugin-router": "^4.5.11",
    "@vue/cli-plugin-vuex": "^4.5.11",
    "@vue/cli-service": "^4.5.11",
    "babel-eslint": "^10.1.0",
    "cross-env": "^7.0.3",
    "dotenv": "^8.2.0",
    "eslint": "^7.20.0",
    "eslint-plugin-vue": "^7.6.0",
    "husky": "^5.1.1",
    "lint-staged": "^10.5.4",
    "nodemon": "^2.0.7",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.2.1",
    "sass": "^1.32.8",
    "sass-loader": "^10.1.1",
    "tar": "^6.1.0",
    "terser": "^5.6.0",
    "vue-svg-loader": "^0.16.0",
    "vue-template-compiler": "~2.6.12"
  }
}
