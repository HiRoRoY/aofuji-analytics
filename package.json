{
  "name": "goose-analytics",
  "version": "0.2.0",
  "private": true,
  "scripts": {
    "lint": "vue-cli-service lint",
    "dev:web": "cross-env NODE_ENV=development vue-cli-service serve",
    "dev:api": "cross-env NODE_ENV=development nodemon --watch api  --watch utils --watch .env.development ./api/index.js",
    "dev": "cross-env NODE_ENV=development npm-run-all -p dev:api dev:web",
    "build:web": "cross-env NODE_ENV=production vue-cli-service build --modern",
    "build": "cross-env NODE_ENV=production npm-run-all -p build:web",
    "start": "cross-env NODE_ENV=production node ./api/index.js"
  },
  "dependencies": {
    "compression": "^1.7.4",
    "connect-history-api-fallback": "~1.6.0",
    "express": "~5.0.0-alpha.8",
    "helmet": "~4.2.0",
    "mongoose": "~5.10.16",
    "normalize.css": "~8.0.1",
    "on-headers": "~1.0.2",
    "vue": "~2.6.12",
    "vue-router": "~3.4.9",
    "vuex": "~3.6.0"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "~4.5.9",
    "@vue/cli-plugin-eslint": "~4.5.9",
    "@vue/cli-plugin-router": "~4.5.9",
    "@vue/cli-plugin-vuex": "~4.5.9",
    "@vue/cli-service": "~4.5.9",
    "babel-eslint": "^10.1.0",
    "cross-env": "^7.0.2",
    "dotenv": "^8.2.0",
    "eslint": "^7.14.0",
    "eslint-plugin-vue": "^7.1.0",
    "nodemon": "^2.0.6",
    "npm-run-all": "^4.1.5",
    "sass": "^1.29.0",
    "sass-loader": "^10.1.0",
    "vue-template-compiler": "~2.6.12"
  }
}
