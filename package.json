{
  "name": "goose-analytics",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "lint": "vue-cli-service lint",
    "dev:web": "cross-env NODE_ENV=development vue-cli-service serve",
    "dev:api": "cross-env NODE_ENV=development nodemon --watch api --watch .env.development ./api/index.js",
    "dev": "cross-env NODE_ENV=development npm-run-all -p dev:api dev:web",
    "build:web": "cross-env NODE_ENV=production vue-cli-service build",
    "build": "cross-env NODE_ENV=production npm-run-all -p build:web",
    "start": "cross-env NODE_ENV=production node ./api/index.js"
  },
  "dependencies": {
    "express": "~5.0.0-alpha.8",
    "vue": "~3.0.2"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "~4.5.9",
    "@vue/cli-plugin-eslint": "~4.5.9",
    "@vue/cli-service": "~4.5.9",
    "@vue/compiler-sfc": "~3.0.2",
    "babel-eslint": "^10.1.0",
    "cross-env": "^7.0.2",
    "dotenv": "^8.2.0",
    "eslint": "^7.14.0",
    "eslint-plugin-vue": "^7.1.0",
    "nodemon": "^2.0.6",
    "npm-run-all": "^4.1.5"
  }
}
